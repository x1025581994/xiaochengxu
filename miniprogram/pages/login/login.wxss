.login_title{
  font-size: 60rpx;
  text-align: center;
  margin-bottom: 60rpx;
}
.login_model{
  width: 680rpx;
  padding: 100rpx 0rpx;
  margin: 60rpx auto;
  box-sizing: border-box;
  box-shadow: 5rpx 20rpx 50rpx #888888;
  border-radius: 15rpx;
  background: transparent;
}
.content {
 width: 100%;
 height: auto;
 padding: 0 50rpx;
 box-sizing: border-box;
}
.phone-box {
 width: 100%;
 height: 89rpx;
 border-bottom: 1rpx solid #b1a9a9;
 display: flex;
 flex-direction: row;
 justify-content: flex-start;
 align-items: center;
}
.phone {
 color: #333;
 margin-right: 60rpx;
 font-size: 38rpx;
}
.number {
 color: #333;
 font-size: 38rpx;
 width: 70%;
}
.getNum {
 width: 30%;
 height:55rpx;
 background:rgba(248, 112, 57, 1);
 border-radius:8rpx;
 font-size:28rpx;
 font-family:PingFang-SC-Medium;
 color:rgba(255, 255, 255, 1);
 line-height:48rpx;
 margin-right:36rpx;
 text-align:center;
}
.submit {
 width: 480rpx;
 height: 80rpx;
 background: rgba(248, 112, 57, 1);
 border-radius: 8rpx;
 margin-top: 80rpx;
 color: #fff;
 font-size: 32rpx;
}